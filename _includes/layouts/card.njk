<script>
		let bgColor = "#{{ renk }}";
		function convertHexToRgbA(hex) {
			let c;
			if (/^#([A-Fa-f0-9]{3}){1,2}$/.test(hex)) {
				c = hex.substring(1).split('');
				if (c.length == 3) {
					c = [c[0], c[0], c[1], c[1], c[2], c[2]];
				}
				c = `0x${c.join('')}`;
				return `rgba(${[(c >> 16) & 255, (c >> 8) & 255, c & 255].join(',')},0.15)`;
			}
		}
		(function () {
			const bg = convertHexToRgbA(bgColor);
			const list = Array.from(document.getElementsByClassName('svg-container'));
			list.map((element) => element.style.backgroundColor = bg)
		})();

	window.onload = function () {
